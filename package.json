{
    "name": "kf-uikit",
    "version": "0.0.0",
    "description": "Kids First Design System",
    "main": "dist/index.js",
    "scripts": {
        "prepare": "rm -rf dist && mkdir dist && babel src --out-dir dist",
        "storybook": "start-storybook -p 9001 -c .storybook",
        "storybook:build": "build-storybook -c .storybook -o ./storybook-static/$npm_package_version && node versionBump.js",
        "storybook:build:serve": "build-storybook -c .storybook -o ./storybook-static/$npm_package_version && node versionBump.js && cd ./storybook-static && python -m SimpleHTTPServer",
        "test": "jest",
        "test:update": "jest --updateSnapshot",
        "build": "BABEL_ENV=static node build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kids-first/kf-uikit.git"
    },
