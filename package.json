{
    "name": "kf-uikit",
    "version": "0.0.0",
    "description": "Kids First Design System",
    "main": "dist/index.js",
    "scripts": {
        "build:components": "rm -rf ./dist/* && babel src --out-dir ./dist",
        "compile:styles-config": "babel ./tailwind.config.js --out-file ./.tmp/tailwind.babel.js ",
        "build:styles": "yarn compile:styles-config && webpack --progress --config=webpack.config.js && rm -rf ./.tmp/*",
        "build": "yarn build:components && yarn build:styles",
        "storybook": "start-storybook -p 9001 -c .storybook",
        "storybook:build": "build-storybook -c .storybook -o ./storybook-static/",
        "storybook:build:serve": "build-storybook -c .storybook -o ./storybook-static/$npm_package_version && node versionBump.js && cd ./storybook-static && python -m SimpleHTTPServer"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kids-first/kf-uikit.git"
    },
    "bugs": {
        "url": "https://github.com/kids-first/kf-uikit/issues"
    },
    "devDependencies": {
        "@babel/cli": "^7.0.0-rc.1",
        "@babel/core": "^7.0.0-rc.1",
        "@babel/plugin-proposal-export-default-from": "^7.0.0-rc.1",
        "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.53",
        "@babel/plugin-transform-template-literals": "^7.0.0-beta.53",
        "@babel/preset-env": "^7.0.0-beta.56",
        "@babel/preset-react": "^7.0.0-beta.56",
        "@buildit/storybook-addon-versions": "^0.4.3",
        "@emotion/babel-plugin-core": "^0.6.6",
        "@storybook/addon-a11y": "^3.4.8",
        "@storybook/addon-actions": "^3.4.8",
        "@storybook/addons": "^3.4.8",
        "autoprefixer": "^9.1.1",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "^8.2.6",
        "babel-loader": "^7.1.5",
        "babel-plugin-dynamic-import-node-babel-7": "^2.0.7",
        "babel-plugin-react-docgen": "^2.0.0-rc.1",
        "babel-plugin-transform-export-default": "^7.0.0-alpha.20",
        "cross-env": "^5.1",
        "css": "^2.2.3",
        "css-to-cssinjs": "^1.3.2",
        "eslint": "^4.19.1",
        "eslint-config-airbnb": "^17.0.0",
        "eslint-config-prettier": "^2.9.0",
        "eslint-loader": "^2.1.0",
        "eslint-plugin-import": "^2.13.0",
        "eslint-plugin-jsx-a11y": "^6.1.1",
        "eslint-plugin-prettier": "^2.6.2",
        "eslint-plugin-react": "^7.10.0",
        "extract-text-webpack-plugin": "^3.0.2",
        "flat": "^4.1.0",
        "mini-css-extract-plugin": "^0.4.1",
        "postcss": "^6.0.14",
        "postcss-loader": "^2.0.8",
        "prettier": "^1.14.0",
        "react-docgen": "^2.21.0",
        "style-loader": "^0.19.0",
        "tailwindcss": "^0.6.5",
        "webpack": "^4.16.5",
        "webpack-command": "^0.4.1"
    },
    "dependencies": {
        "@storybook/addon-info": "^3.4.8",
        "@storybook/addon-knobs": "^3.4.8",
        "@storybook/react": "^3.4.8",
        "babel-polyfill": "^6.26.0",
        "chroma-js": "^1.3.7",
        "lodash": "^4.17.10",
        "n": "^2.1.12",
        "npm": "^6.3.0",
        "postcss-easy-import": "^3.0.0",
        "postcss-nested": "^3.0.0",
        "prop-types": "^15.6.2",
        "react": "^16.4.1",
        "react-dom": "^16.4.1"
    }
}
